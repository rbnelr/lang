func fib_iter () {
	n : int;
	{
		a := 5;
		b := 10;
		n = a * b - 5;
	}

	a := 0;
	b := 1;

	printf("Fibbonaci: {i} {i}", a, b);

	for i := 0; i < n-2; i++ {
		c := a + b;
		printf(" {i}", c);
		a = b;
		//break;
		b = c;
	}

	printf("\n");
	return;
}

fib_iter();

func sqrt (square : int) = (sqrt : int) { 
	sqrt = -1;
	for i:=0; i<100; i++ {
		if i*i == square {
			sqrt = i;
			break;
		}
	}
	printf("sqrt({i}) is {i}\n", square, sqrt);
}

sqrt(625); // 25^2

