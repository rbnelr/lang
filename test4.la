func T () = (b : bool) {
	printf("T ");
	return true;
}
func F () = (b : bool) {
	printf("F ");
	b = false;
}

printf("F() && T():\n  ");
if F() && T() {
	printf(" -> true!\n");
}
printf("\n");

printf("T() && F():\n  ");
if T() && F() {
	printf(" -> true!\n");
}
printf("\n");

printf("T() || F():\n  ");
if T() || F() {
	printf(" -> true!\n");
}
printf("\n");

printf("!T() && F():\n  ");
if !T() && F() {
	printf(" -> true!\n");
}
printf("\n");

printf("!T() & F():\n  ");
if !T() & F() {
	printf(" -> true!\n");
}
